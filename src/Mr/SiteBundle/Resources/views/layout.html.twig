<!doctype html>
<html class="no-js" lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Foundation | Welcome</title>

    {% stylesheets
        'bundles/mrsite/css/normalize.css'
        'bundles/mrsite/css/foundation.css'
        'bundles/mrsite/css/mr.css'
        filter='cssrewrite' %}
        <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}

    <script src="{{ asset('bundles/mrsite/js/modernizr.js') }}"></script>
</head>
<body>

<nav class="top-bar" data-topbar>
    <ul class="title-area">
        <li class="name">
            <h1><a href="#">Mr</a></h1>
        </li>
    </ul>
</nav>

<header class="sky">
    {% include  '@MrSiteBundle/img/svg/pousse.s1.svg' %}
</header>


</svg>

<nav class="ground">

</nav>

<script src="{{ asset('bundles/mrsite/js/jquery.js') }}"></script>
<script src="{{ asset('bundles/mrsite/js/foundation.min.js') }}"></script>
<script src="{{ asset('bundles/mrsite/js/snap.svg-min.js') }}"></script>

<script>
    $(document).foundation();

    var s = Snap("#sky");

    Snap.load("{{ asset('bundles/mrsite/img/svg/pousse.s1.svg') }}", function (f) {
        g = f.select("g");
        s.append(g);
    });
</script>
</body>
</html>
